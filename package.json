{
    "name": "modelbrouwers",
    "version": "2.1.3",
    "description": "Bundle for Modelbrouwers.nl community website",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "NODE_ENV=production webpack",
        "dev": "webpack -w",
        "prettier-watch": "onchange '**/*.js' -e 'src/static/bundles/**/*.js' -- prettier --write {{changed}}",
        "generate": "plop --plopfile src/js/scripts/generator/index.js",
        "makemessages-en": "formatjs extract 'src/js/**/*.js' --format src/js/i18n-formatter.js --out-file src/js/locale/locales/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
        "makemessages-nl": "formatjs extract 'src/js/**/*.js' --format src/js/i18n-formatter.js --out-file src/js/locale/locales/nl.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
        "makemessages-de": "formatjs extract 'src/js/**/*.js' --format src/js/i18n-formatter.js --out-file src/js/locale/locales/de.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
        "makemessages": "npm run makemessages-nl && npm run makemessages-en && npm run makemessages-de",
        "compilemessages": "formatjs compile-folder --ast src/js/locale/locales src/js/locale/compiled",
        "start": "webpack -w"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/modelbrouwers/modelbrouwers.git"
    },
    "keywords": [
        "phpbb",
        "django",
        "forum"
    ],
    "author": "Sergei Maertens <sergeimaertens@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/modelbrouwers/modelbrouwers/issues"
    },
    "homepage": "https://github.com/modelbrouwers/modelbrouwers",
    "dependencies": {
        "@babel/polyfill": "^7.2.5",
        "bootstrap": "^3.3.7",
        "bootstrap-datepicker": "^1.9.0",
        "bootstrap-select": "^1.13.8",
        "classnames": "^2.2.6",
        "consumerjs": "^3.1.0",
        "fine-uploader": "^5.16.2",
        "font-awesome": "^4.7.0",
        "handlebars": "^4.0.12",
        "immer": "^9.0.1",
        "insert-text-at-cursor": "^0.1.1",
        "jquery": "^3.5.1",
        "mobx": "^5.8.0",
        "mobx-react": "^5.4.3",
        "perfect-scrollbar": "^1.4.0",
        "prop-types": "^15.6.2",
        "react": "^16.7.0",
        "react-dom": "^16.7.0",
        "react-dropzone-uploader": "^2.11.0",
        "react-intl": "^5.25.0",
        "react-select": "^3.1.0",
        "react-use": "^15.0.1",
        "string.prototype.startswith": "^0.2.0",
        "typeahead.js": "^0.11.1",
        "urijs": "^1.19.1",
        "use-immer": "^0.5.1"
    },
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.2.3",
        "@babel/plugin-proposal-decorators": "^7.2.3",
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/plugin-transform-runtime": "^7.17.0",
        "@babel/preset-env": "^7.3.1",
        "@babel/preset-react": "^7.0.0",
        "@formatjs/cli": "^4.8.3",
        "autoprefixer": "^10.4.4",
        "babel-loader": "^8.0.5",
        "babel-plugin-formatjs": "^10.3.19",
        "css-loader": "^6.7.1",
        "cssnano": "^5.1.7",
        "lodash.camelcase": "^4.3.0",
        "lodash.capitalize": "^4.2.1",
        "lodash.kebabcase": "^4.1.1",
        "lodash.upperfirst": "^4.3.1",
        "mini-css-extract-plugin": "^2.6.0",
        "onchange": "^7.1.0",
        "plop": "^2.2.0",
        "postcss-loader": "^6.2.1",
        "postcss-selector-lint": "^1.0.2",
        "prettier": "^2.6.2",
        "sass": "^1.50.1",
        "sass-loader": "^12.6.0",
        "uglify-js": "^2.4.23",
        "webpack": "^5.72.0",
        "webpack-cli": "^4.9.2",
        "yargs": "^17.4.1"
    }
}
