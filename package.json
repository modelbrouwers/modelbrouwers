{
  "name": "modelbrouwers",
  "version": "2.12.0",
  "description": "Bundle for Modelbrouwers.nl community website",
  "main": "index.js",
  "scripts": {
    "test": "test-storybook",
    "build": "NODE_ENV=production webpack",
    "dev": "webpack -w --devtool eval-source-map",
    "format": "prettier --write 'src/{js,sass}/**/*.{js,ts,tsx,scss,mdx}'",
    "checkformat": "prettier --check 'src/{js,sass}/**/*.{js,ts,tsx,scss,mdx}'",
    "makemessages-en": "formatjs extract 'src/js/**/*.{js,ts,tsx}' --format src/js/i18n-formatter.js --out-file src/js/locale/locales/en.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "makemessages-nl": "formatjs extract 'src/js/**/*.{js,ts,tsx}' --format src/js/i18n-formatter.js --out-file src/js/locale/locales/nl.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "makemessages-de": "formatjs extract 'src/js/**/*.{js,ts,tsx}' --format src/js/i18n-formatter.js --out-file src/js/locale/locales/de.json --id-interpolation-pattern '[sha512:contenthash:base64:6]'",
    "makemessages": "npm run makemessages-nl && npm run makemessages-en && npm run makemessages-de",
    "compilemessages": "formatjs compile-folder --ast src/js/locale/locales src/js/locale/compiled",
    "start": "webpack -w --devtool eval-source-map",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/modelbrouwers/modelbrouwers.git"
  },
  "keywords": [
    "phpbb",
    "django",
    "forum"
  ],
  "author": "Sergei Maertens <sergeimaertens@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/modelbrouwers/modelbrouwers/issues"
  },
  "homepage": "https://github.com/modelbrouwers/modelbrouwers",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "bootstrap": "^3.4.1",
    "bootstrap-datepicker": "^1.9.0",
    "bootstrap-select": "^1.13.8",
    "classnames": "^2.2.6",
    "clsx": "^2.1.1",
    "fine-uploader": "^5.16.2",
    "font-awesome": "^4.7.0",
    "formik": "^2.4.6",
    "i18n-iso-countries": "^4.1.1",
    "immer": "^9.0.1",
    "insert-text-at-cursor": "^0.1.1",
    "jquery": "^3.5.1",
    "perfect-scrollbar": "^1.4.0",
    "prop-types": "^15.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-dropzone-uploader": "^2.11.0",
    "react-intl": "^6.6.8",
    "react-router": "^7.5.2",
    "react-select": "^5.3.1",
    "react-use": "^17.3.2",
    "string.prototype.startswith": "^0.2.0",
    "swiper": "^8.1.4",
    "typeahead.js": "^0.11.1",
    "urijs": "^1.19.1",
    "use-immer": "^0.8.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.24.7",
    "@chromatic-com/storybook": "^3.2.4",
    "@formatjs/cli": "^6.2.12",
    "@formatjs/ts-transformer": "^3.13.14",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-interactions": "^8.6.12",
    "@storybook/addon-links": "^8.6.12",
    "@storybook/addon-styling-webpack": "^1.0.1",
    "@storybook/addon-webpack5-compiler-babel": "^3.0.5",
    "@storybook/blocks": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/react-webpack5": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@storybook/test-runner": "^0.22.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/lodash": "^4.17.15",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.0.5",
    "babel-plugin-formatjs": "^10.5.16",
    "css-loader": "^6.7.1",
    "cssnano": "^5.1.7",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "^2.6.0",
    "msw": "^2.3.1",
    "msw-storybook-addon": "^2.0.4",
    "onchange": "^7.1.0",
    "postcss-loader": "^6.2.1",
    "postcss-selector-lint": "^1.0.2",
    "prettier": "^3.5.3",
    "sass": "^1.50.1",
    "sass-loader": "^12.6.0",
    "storybook": "^8.6.12",
    "storybook-addon-remix-react-router": "^4.0.1",
    "storybook-react-intl": "^3.2.3",
    "ts-loader": "^9.5.1",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.4.5",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "yargs": "^17.4.1"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "prettier": {
    "$schema": "http://json.schemastore.org/prettierrc",
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ],
    "arrowParens": "avoid",
    "bracketSpacing": false,
    "bracketSameLine": false,
    "jsxSingleQuote": false,
    "printWidth": 100,
    "proseWrap": "always",
    "quoteProps": "as-needed",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "useTabs": false,
    "importOrder": [
      "^((albums|builds|components|data|forms|forum|kitreviews|kits|locale|ponyjs|router|scripts|shop|storybook)/(.*)|(constants|csrf|fallback-img|forum|i18n|index|mobile-nav))$",
      "^@/.*",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  }
}
